<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>student Information</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    table, th, td {
      border: 1px solid black;
    }
    th, td {
      padding: 8px;
      text-align: left;
    }
  </style>
</head>
<body>

  <h1>student Information</h1>

  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Full Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Birth Date</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Country</th>
        <th>City</th>
        <th>Region</th>
        <th>Postal Code</th>
      </tr>
    </thead>
    <tbody>
      <% students.forEach(function(student) { %>
        <tr>
          <td><%= student._id %></td>
          <td><%= student.full_name %></td>
          <td><%= student.email %></td>
          <td><%= student.phone %></td>
          <td><%= student.birth_date %></td>
          <td><%= student.gender %></td>
          <td><%= student.address %> <%= student.address_line2 %></td>
          <td><%= student.country %></td>
          <td><%= student.city %></td>
          <td><%= student.region %></td>
          <td><%= student.postal_code %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

</body>
</html>
